<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="tsweets" id="1">
        <createTable tableName="cardholder">
            <column name="id" type="bigint">
                <constraints primaryKey="true" primaryKeyName="cardholder_id_pk"/>
            </column>
            <column name="env_id" type="int"/>
            <column name="external_id" type="bigint"/>
            <column name="first_name" type="varchar"/>
            <column name="middle_name" type="varchar"/>
            <column name="last_name" type="varchar"/>
            <column name="dob" type="date"/>
            <column name="ssn" type="varchar"/>
            <column name="home_street1" type="varchar"/>
            <column name="home_street2" type="varchar"/>
            <column name="home_city" type="varchar"/>
            <column name="home_state" type="varchar"/>
            <column name="home_postal_code" type="varchar"/>
            <column name="ship_street1" type="varchar"/>
            <column name="ship_street2" type="varchar"/>
            <column name="ship_city" type="varchar"/>
            <column name="ship_state" type="varchar"/>
            <column name="ship_postal_code" type="varchar"/>
            <column name="phone_number" type="varchar"/>
            <column name="email" type="varchar"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
